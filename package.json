{
  "name": "flight-mission",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/pro-layout": "^7.22.3",
    "@antv/l7": "^2.22.5",
    "@antv/l7-maps": "^2.22.5",
    "@deck.gl/extensions": "^9.1.8",
    "@deck.gl/layers": "^9.1.8",
    "@deck.gl/mapbox": "^9.1.8",
    "@geoman-io/maplibre-geoman-free": "^0.3.0",
    "@hyvilo/maplibre-gl-draw": "^1.0.0",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@maplibre/maplibre-gl-geocoder": "^1.8.0",
    "@maptiler/weather": "^3.0.1",
    "@sakitam-gis/maplibre-wind": "^2.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "@turf/turf": "^7.2.0",
    "antd": "^5.24.5",
    "axios": "^1.7.9",
    "canvas": "^3.1.1",
    "deck.gl": "^9.1.8",
    "heap": "^0.2.7",
    "lru-cache": "^11.1.0",
    "maplibre-gl": "^5.3.0",
    "next": "^15.3.4",
    "node-fetch": "^3.3.2",
    "p-limit": "^6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "weatherlayers-gl": "^2025.1.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron": "^34.2.0",
    "electron-builder": "^23.6.0"
  },
  "main": "main.js",
  "scripts": {
    "dev:next": "next dev",
    "dev:electron": "cross-env NEXT_PUBLIC_PLATFORM=desktop ELECTRON_RENDERER_URL=http://localhost:3000 electron .",
    "dev": "concurrently \"pnpm run dev:next\" \"pnpm run dev:electron\"",
    "build:renderer": "next build && next export -o out/renderer",
    "build:electron": "babel electron/ -d out",
    "build": "pnpm run build:renderer && pnpm run build:electron",
    "dist": "pnpm run build && electron-builder",
    "dist:win": "pnpm run build && electron-builder --win",
    "dist:mac": "pnpm run build && electron-builder --mac",
    "dist:linux": "pnpm run build && electron-builder --linux"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "appId": "com.yourcompany.flightmission",
    "productName": "FlightMission",
    "directories": {
      "output": "dist"
    },
    "files": [
      "build/**/*",
      "main.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    }
  }
}